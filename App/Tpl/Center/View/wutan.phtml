function showXY()
{
	var aa = $("chichun").value
	if (aa!=""){
		var aa = aa.split('*');
		$("xx").value = aa[0];
		$("yy").value = aa[1];
	}
	
	if ($("isdiy").checked){
		$("dis").style.display = "block";
		$("chichun").disabled=true;
	}else{
		$("chichun").disabled=false;
		$("dis").style.display = "none";
		
	}
}
