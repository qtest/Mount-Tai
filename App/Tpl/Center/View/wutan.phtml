	}
	
}
//单选单双面选择
function dxian(the){
	if (the.checked){
		var obj=$$("input");   
		for(i=0;i<obj.length;i++){     
			if (obj[i].type=="checkbox" && obj[i].checked && obj[i].value.indexOf('面')>0){
				obj[i].checked =false;
			}
	
		} 
		the.checked=true;
	}
}
